<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>

        <class name="twowayRMI.Professeur" table="prof">
                <id name="idProf" column="idProf" type="long">
                        <generator class="native"/>
                </id>
                <property name="nom"/>
                <property name="prenom"/>
                <property name="mail"/>
        </class>
        
</hibernate-mapping>